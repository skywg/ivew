.wrapper-container{
    width: 1197px;
    margin: 0 auto;
}
.main{
    margin-top: 10px;
}
.main-l{
    width: 210px;
    margin-right: 10px;
}
.main-l h2{
    text-align: center;
    margin-top: 18px;
    margin-bottom: 30px;
    font-size: 20px;
    font-weight: normal;
}

.main-l p{
   font-size: 16px;
    color: #333;
    margin-top: -5px;
}
.main-l span{
    font-size: 12px;
    color: #999999;
}

.yingyong{
    border: 1px solid #ededed;
    padding-bottom: 26px;
}


.main-c{
    width: 644px;
    margin-right: 10px;
}
.main-c-1{
    border: 1px solid #ededed;
    padding-bottom: 24px;
}
.main-c-1 p{
    line-height: 24px;
}
.user-img{
    width: 74px;
    height: 74px;
    margin: 20px auto;
    background: url("../img/user-img.png") no-repeat ;
}
.main-c-2{
    margin-top: 10px;
}
.mian-c-title{
    height: 50px;
    background: #f8f8f8;
    padding-top: 14px;

}
.mian-c-title h3{
    font-size: 16px;
    color: #333;
    padding-left: 12px;
    border-left: 4px solid #51ad79;
    font-weight: normal;
}


.main-c-1-r{
    float: right;
}
.main-c-1-r h4{
    font-size: 20px;
    color: #333;
    font-weight: normal;
}
.main-c-1-r span{
    font-size: 12px;
    color: #999;
}
.main-c-1-r ul {
    margin: 24px;
}
.main-c-1-r ul li{
    float: left;
    margin-left: 30px;
    text-align: center;
}

.main-r {
    width: 266px;
}

.main-r-1{
    padding: 18px 32px 22px 32px;
    border: 1px solid #ededed;
    margin-bottom: 10px;
}

.app-list p{
    font-size: 16px;
    color: #333;
    margin-top: -5px;
}
.app-list span{
    font-size: 12px;
    color: #999999;
}
.main-r-2{
    width: 210px;
    padding: 18px 0;
    border-left: 1px solid #ededed;
    margin-bottom: 10px;
}
.main-r-2 ul li {
    font-size: 14px;
    text-align: center;
    line-height: 40px;
    display: block;

}
.main-r-2 ul li a{
    font-size: 14px;
    text-align: center;
    line-height: 40px;
    display: block;
    color: #333;

}
.main-r-2 ul li:hover{
    background: #fafafa;
    border-left: 4px solid #51ad79;
    color: #51ad79;

}
.main-r-2 ul li a:hover {
    color: #51ad79;

}
