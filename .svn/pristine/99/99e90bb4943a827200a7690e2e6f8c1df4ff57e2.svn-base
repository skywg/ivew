
//字体图标
//-----------------------------------------------------
$font-icon-path              : '../../font/iconfont';
$font-icon-name              : 'iconfont';
$font-svg-id                 : 'iconfont';

// @include font-face($font-icon-name,$font-icon-path,normal,normal);
@font-face {
    font-family: iconfont;
    font-weight: 400;
    font-style: normal;
    src: url('#{$font-icon-path}.eot');
    src: url('#{$font-icon-path}.eot?#iefix') format('eot'), 
         url('#{$font-icon-path}.svg#iconfont') format('svg'), 
         url('#{$font-icon-path}.woff') format('woff'), 
         url('#{$font-icon-path}.ttf') format('truetype')
}

[class^="icon-"],
[class*=" icon-"]{
    position: relative;
    line-height: 1;
    &:before{
        display: inline-block;
        vertical-align: middle;
        font-family: $font-icon-name;
        font-style: normal;
        -webkit-font-smoothing: antialiased;
        -moz-osx-font-smoothing: grayscale;
    }
}

