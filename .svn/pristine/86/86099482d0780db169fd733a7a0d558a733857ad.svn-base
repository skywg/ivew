            } else {
                search.defOpt = [];
            }
        },
        // 排序处理
        handleSort(item,index){
            this.filterBtn.forEach(item => item.type='ghost')
            item.type = 'primary'
            if(index === 1){
                this.auctionList = this.auctionList.sort((a, b) => a.price1 < b.price1 ? -1 : 1)
            }else if(index === 2){
                this.auctionList = this.auctionList.sort((a, b) => a.pople < b.pople ? 1 : -1)
            }
        },
        // 前进
        handleForward(){
            this.page.current++
            if(this.auctionList.length < 8) this.auctionList = [...this.auctionList, ...this.auctionList]
        },
        // 后退
        handleBack(){
            if(this.page.current > 1){
                this.page.current--
                this.auctionList = this.auctionList.slice(0,4)
            }

        }
    }
}
</script>
