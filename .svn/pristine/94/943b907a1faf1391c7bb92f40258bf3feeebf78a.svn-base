    border-left:1px solid #ccc;
    padding-left:40px;
  }
  .xg-content div{
    margin-top:10px;
  }
  .footPager{
    margin-top:20px;
  }
  .login-r{
    position:absolute;
    top:0;
    right:44px;
  }
  button.login{
