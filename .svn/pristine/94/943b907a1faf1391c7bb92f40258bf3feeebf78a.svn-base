    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 3; //文本行数
    overflow: hidden;
  }
  .resultList-infor div{
    margin-top:8px;
    font-size:14px;
    color:#333;
  }

  .clear {
    clear:both;
    overflow:hidden;
  }
  .search-xgyy li{
    float:left;
    margin:26px 28px 26px 0;
    text-align:center;
  }
  .xg-content{
    float:left;
