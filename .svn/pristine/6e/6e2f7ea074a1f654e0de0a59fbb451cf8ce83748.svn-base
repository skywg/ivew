                if (txt == "所有人可见") {
                    txt = "0"
                } else if (txt == "仅好友可见") {
                    txt = "1"
                } else if (txt == "仅自己可见") {
                    txt = "2"
                } else if (txt == "仅指定组可见") {
                    txt = "3"
                }
                axios({
                    method: 'post',
                    url: 'http://192.168.13.44:1111/mydynamic/saveMydynamic',
                    data: {
                        title: this.mydynamic.title,
                        type: this.mydynamic.type,
                        category: this.mydynamic.category,
                        source: this.mydynamic.source,
                        district: this.mydynamic.district,
                        goodsname: this.mydynamic.goodsname,
                        servicename: this.mydynamic.servicename,
                        label: this.mydynamic.label,
                        species: this.mydynamic.species,
                        summary: this.mydynamic.summary,
                        industry: this.mydynamic.industry,
                        prior: this.mydynamic.title,
                        content: UE.getEditor('editor').getContent(),
                        security: txt
                    }
                }).then(response => {
                    this.$Message.success('发布成功!')
                    window.location.reload();

                }).catch(function (error) {
                    this.$Message.success('发布失败!');
                    console.log(error)
                })

