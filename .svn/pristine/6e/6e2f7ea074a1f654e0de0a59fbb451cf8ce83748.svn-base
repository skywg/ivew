            /**
             * 点赞
             * */
            dianzan(id) {
                axios.get('http://192.168.13.44:1111/mydynamic/saveZan?id=' + id)
                    .then(response => {
                        if (response.data.flag == '1') {
                            var div = $("#" + id).prev()
                            $.each($(div).find("span"), function (i, span) {
                                var txt = $(span).text()
                                if (txt.indexOf("赞") > -1) {
                                    var html = $(span).html()
                                    html = html.replace(txt, "")
                                    $(span).html(html + "赞" + response.data.thumbUpNum)
                                }
                            })
                        }
                    }).catch(function (error) {
                    console.log(error)
                })
            },
